{
    "name": "widoz/wordpress-template",
    "description": "WordPress template interface",
    "type": "library",
    "keywords": [
        "wordpress",
        "psr",
        "autoloader",
        "composer"
    ],
    "homepage": "https://github.com/widoz/wordpress-template",
    "license": "GPL-2.0-only",
    "authors": [
        {
            "name": "Guido Scialfa",
            "email": "dev@guidoscialfa.com",
            "role": "Developer"
        }
    ],
    "minimum-stability": "dev",
    "autoload": {
        "classmap": [
            "src/"
        ],
        "psr-4": {
            "WordPressTemplate\\": "src"
        }
    },
    "require": {
        "php": ">=7.1"
    },
    "require-dev": {
        "phpunit/phpunit": "~6.5.5",
        "brain/monkey": "dev-master",
        "inpsyde/php-coding-standards": "^0.13.3"
    }
}
